---
const links = [
  { href: '/', label: 'Home' },
  { href: '/work', label: 'Work' },
  { href: '/about', label: 'About' },
  { href: '/links', label: 'Links' },
]
---
<nav class="sticky top-0 z-40 bg-white/80 backdrop-blur border-b border-neutral-200">
  <div class="mx-auto max-w-6xl px-6 h-14 flex items-center justify-between">
    <a href="/" class="font-semibold tracking-tight">Ariane</a>
    <ul class="flex gap-6 text-sm">
      {links.map(l => (
        <li>
          <a class="hover:underline" href={l.href}>{l.label}</a>
        </li>
      ))}
    </ul>
  </div>
</nav>