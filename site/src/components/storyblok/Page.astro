---
import { storyblokEditable, StoryblokComponent } from '@storyblok/astro'

interface Props {
  blok: {
    body: any[]
    component: string
  }
}

const { blok } = Astro.props
---

<main {...storyblokEditable(blok)}>
  {blok.body?.map((nestedBlok) => (
    <StoryblokComponent blok={nestedBlok} />
  ))}
</main>