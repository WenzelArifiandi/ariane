{
  "cleanUrls": true,
  "trailingSlash": false,
  "headers": [
    {
      "source": "/_astro/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "rewrites": [
    { "source": "/about", "destination": "https://wenzelarifiandi.com/about" },
    { "source": "/links", "destination": "https://wenzelarifiandi.com/links" },
    { "source": "/work", "destination": "https://wenzelarifiandi.com/work" },
    { "source": "/work/:slug", "destination": "https://wenzelarifiandi.com/work/:slug" },
    { "source": "/api/auth/:path*", "destination": "https://wenzelarifiandi.com/api/auth/:path*" }
  ]
}
