# Example terraform.tfvars file for local development
# Copy this to terraform.tfvars and fill in your actual values
# terraform.tfvars is gitignored for security

# Cloudflare API token (required)
# Get this from: https://dash.cloudflare.com/profile/api-tokens
cloudflare_api_token = "your_cloudflare_api_token_here"

# Cipher OIDC credentials (required)
# Get these from your ZITADEL Cipher OIDC application configuration
cipher_client_id     = "your_cipher_oidc_client_id_here"
cipher_client_secret = "your_cipher_oidc_client_secret_here"

# Cipher issuer URL (optional - defaults to https://cipher.wenzelarifiandi.com)
# cipher_issuer_url = "https://cipher.wenzelarifiandi.com"